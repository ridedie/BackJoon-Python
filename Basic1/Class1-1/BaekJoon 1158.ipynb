   "source": [
    "\n",
    "from collections import deque\n",
    "\n",
    "N, K = map(int, input().split())\n",
    "result = []\n",
    "deq = deque([i+1 for i in range(N)])\n",
    "\n",
    "while deq:\n",
    "  for _ in range(K-1):\n",
    "    deq.append(deq.popleft())\n",
    "\n",
    "  result.append(deq.popleft())\n",
    "\n",
    "print(str(result).replace('[', '<').replace(']', '>'))"
   ]
  }
