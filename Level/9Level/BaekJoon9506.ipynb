   "source": [
    "while True:\n",
    "    n = int(input())\n",
    "    if n == -1:\n",
    "        break;\n",
    "    arr = []\n",
    "    for i in range(1, n):\n",
    "        if n % i == 0:\n",
    "            arr.append(i)\n",
    "    if sum(arr) == n:\n",
    "        print(n, \" = \", \" + \".join(str(i) for i in arr), sep=\"\")\n",
    "    else:\n",
    "        print(n, \"is NOT perfect.\")"
   ]
  }
