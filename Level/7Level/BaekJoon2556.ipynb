   "source": [
    "A = [list(map(int, input().split())) for _ in range(9)]\n",
    "\n",
    "max = 0\n",
    "max_x, max_y = 0, 0\n",
    "\n",
    "for x in range(9):\n",
    "    for y in range(9):\n",
    "        if max <= A[x][y]:\n",
    "            max_x = x+1\n",
    "            max_y = y+1\n",
    "            max = A[x][y]\n",
    "print(max)\n",
    "print(max_x, max_y)"
   ]
  }
