   "source": [
    "A = [list(map(int, input().split())) for _ in range(9)]\n",
    "\n",
    "max = 0\n",
    "x, y = 0, 0\n",
    "\n",
    "for i in range(9):\n",
    "    for j in range(9):\n",
    "        if max <= A[i][j]:\n",
    "            max = A[i][j]\n",
    "            x = i+1\n",
    "            y = j+1\n",
    "print(max)\n",
    "print(x, y)"
   ]
  }
