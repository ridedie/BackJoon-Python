    "N = int(input())\n",
    "\n",
    "G = 0\n",
    "\n",
    "for _ in range(N):\n",
    "    A = input()\n",
    "    E = 0\n",
    "    for i in range(len(A)-1):\n",
    "        if A[i] ! A[i+1]:\n",
    "            C = A[i+1:]\n",
    "            if C.count(A[i]) > 0:\n",
    "                E += 1\n",
    "    if E == 0:\n",
    "        G += 1\n",
    "print(G)"
   ]
